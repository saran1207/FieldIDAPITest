<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.fieldid</groupId>
	<artifactId>fieldid-root</artifactId>
    <version>2014.6.1-SNAPSHOT</version>
	<packaging>pom</packaging>
	<name>Field ID Parent POM</name>
	<modules>
		<module>fid-core</module>
		<module>fid-test-utils</module>
		<module>fid-cert-data</module>
		<module>fid-web</module>
		<module>fid-exporter</module>
		<module>fid-ws</module>
        <module>fid-migrations</module>
        <module>fid-connection-monitor</module>
        <module>fid-index-validator</module>
    </modules>
	<repositories>
		<repository>
			<snapshots>
				<enabled>false</enabled>
			</snapshots>
			<id>central</id>
			<name>libs-release</name>
			<url>http://repo.n4systems.net/artifactory/libs-release</url>
		</repository>
		<repository>
			<snapshots />
			<id>snapshots</id>
			<name>libs-snapshot</name>
			<url>http://repo.n4systems.net/artifactory/libs-snapshot</url>
		</repository>
	</repositories>
	<pluginRepositories>
		<pluginRepository>
			<snapshots>
				<enabled>false</enabled>
			</snapshots>
			<id>central</id>
			<name>plugins-release</name>
			<url>http://repo.n4systems.net/artifactory/plugins-release</url>
		</pluginRepository>
		<pluginRepository>
			<snapshots />
			<id>snapshots</id>
			<name>plugins-snapshot</name>
			<url>http://repo.n4systems.net/artifactory/plugins-snapshot</url>
		</pluginRepository>
	</pluginRepositories>
	<build>
		<pluginManagement>
			<plugins>
				<plugin>
					<artifactId>maven-resources-plugin</artifactId>
					<configuration>
						<encoding>UTF-8</encoding>
					</configuration>
				</plugin>
                <plugin>
                    <groupId>org.jasig.maven</groupId>
                    <artifactId>sass-maven-plugin</artifactId>
                    <version>1.1.0</version>
                </plugin>
			</plugins>
		</pluginManagement>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.6</source>
					<target>1.6</target>
					<encoding>UTF-8</encoding>
				</configuration>
			</plugin>
			<plugin>
				<artifactId>maven-resources-plugin</artifactId>
				<configuration>
					<encoding>UTF-8</encoding>
				</configuration>
			</plugin>
			<plugin>
				<artifactId>maven-deploy-plugin</artifactId>
				<configuration>
					<uniqueVersion>false</uniqueVersion>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<dependencyManagement>
		<dependencies>
			<dependency>
				<groupId>com.fieldid</groupId>
				<artifactId>fid-core</artifactId>
				<version>${pom.version}</version>
			</dependency>
			<dependency>
				<groupId>com.fieldid</groupId>
				<artifactId>fid-ws</artifactId>
				<version>${pom.version}</version>
			</dependency>
			<dependency>
				<groupId>com.fieldid</groupId>
				<artifactId>fid-test-utils</artifactId>
				<version>${pom.version}</version>
			</dependency>
			<dependency>
				<groupId>com.fieldid</groupId>
				<artifactId>fid-cert-data</artifactId>
				<version>${pom.version}</version>
			</dependency>
			<dependency>
				<groupId>com.fieldid</groupId>
				<artifactId>fid-web</artifactId>
				<version>${pom.version}</version>
			</dependency>
			<dependency>
				<groupId>com.fieldid</groupId>
				<artifactId>fid-core</artifactId>
				<version>${pom.version}</version>
				<type>test-jar</type>
			</dependency>
			<dependency>
                <groupId>com.fieldid</groupId>
                <artifactId>fid-migrations</artifactId>
                <version>${pom.version}</version>
            </dependency>
			<dependency>
                <groupId>com.fieldid</groupId>
                <artifactId>fid-connection-monitor</artifactId>
                <version>${pom.version}</version>
            </dependency>
		</dependencies>
	</dependencyManagement>
	<scm>
		<connection>scm:svn:https://fieldid.jira.com/svn/WEB</connection>
		<developerConnection>scm:svn:https://fieldid.jira.com/svn/WEB</developerConnection>
	</scm>
</project>
